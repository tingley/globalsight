<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.globalsight.cxe.entity.filterconfiguration.SidFilter"
        table="sid_filter">
        <id name="id" column="ID" type="long">
            <generator class="native" />
        </id>
        <property name="companyId" column="COMPANY_ID" type="long"
            not-null="true" unique="false" />
        <property name="filterName" length="255" column="FILTER_NAME"
            type="string" not-null="true" unique="false" />
        <property name="filterDescription" length="2000" column="FILTER_DESCRIPTION"
            type="string" not-null="false" unique="false" />
        <property name="configXml" column="CONFIG_XML" type="string"
			not-null="true" unique="false" />
		<property name="type" column="FILTER_TYPE" type="int"
			not-null="true" unique="false" />
		<property name="exclusionFilterId" column="EXCLUSION_FILTER_ID" type="long" />	
    </class>
</hibernate-mapping>